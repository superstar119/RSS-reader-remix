generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(cuid())
  email    String @unique
  password String
  feeds    Feed[]
}

model Feed {
  id     String @id @default(cuid())
  url    String
  userId String
  orderId  Int
  user   User   @relation(fields: [userId], references: [id], onDelete: Cascade)
}
